@model IEnumerable<WebApplication12.Models.Take>

@{
    ViewBag.Title = "Index";
}

<h2>Results</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.marks)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Student.password)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Test.name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.marks)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Student.password)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Test.name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.take_id }) |
            @Html.ActionLink("Details", "Details", new { id=item.take_id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.take_id })
        </td>
    </tr>
}

</table>
